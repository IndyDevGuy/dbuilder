<!DOCTYPE html>
<html lang="en">

    <head>

        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <meta name="description" content="">
        <meta name="author" content="">

        <title>{% block title %}Admin Dashboard{% endblock %} | Disciple Builder</title>

        {% block stylesheets %}
            <link href="{{ asset('build/admin.css') }}" rel="stylesheet">
        {% endblock %}
    </head>

    <body id="page-top">

        {% block navbar %}
            {{ include('layout/admin/nav.html.twig') }}
        {% endblock %}

        <div id="wrapper">
            {% block sidebar %}
                {{ include('layout/admin/sidebar.html.twig') }}
            {% endblock %}
      

            <div id="content-wrapper">

                <div class="container-fluid">
                    {% block breadcrumbs %}
                        {{ include('layout/admin/breadcrumbs.html.twig') }}
                    {% endblock %}

                    {% block body %}

                    {% endblock %}
                </div>
                <footer class="sticky-footer">
                  <div class="container my-auto">
                    <div class="copyright text-center my-auto">
                      <span>Copyright Â© Disciple Builder 2018</span>
                    </div>
                  </div>
                </footer>

            </div>
            <!-- /.content-wrapper -->

        </div>
        <!-- /#wrapper -->

        <!-- Scroll to Top Button-->
        <a class="scroll-to-top rounded" href="#page-top">
            <i class="fas fa-angle-up"></i>
        </a>

        <!-- Javascript block -->
        {% block javascripts %}
            <script src="{{ asset('build/admin.js') }}"></script>
        {% endblock %}
   
    </body>

</html>